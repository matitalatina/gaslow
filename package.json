{
  "name": "gaslow",
  "version": "0.1.0",
  "description": "Italian API price gas stations",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Mattia Natali",
  "license": "MIT",
  "config": {
    "s3BucketName": "gaslow-backend",
    "region": "eu-west-1",
    "cloudFormationStackName": "GaslowServerlessStack",
    "functionName": "GaslowServerlessStack-GaslowLambdaBackend-3JBO773U30Q",
    "accountId": "636133167487"
  },
  "scripts": {
    "start": "npm run serve",
    "build-serverless": "parcel build src/lambda.ts --target node",
    "build": "parcel build src/server.ts --target node",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "check-ts": "tsc --noEmit",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "config": "node ./serverless/scripts/configure.js",
    "deconfig": "node ./serverless/scripts/deconfigure.js",
    "local": "node scripts/local",
    "invoke-lambda": "aws lambda invoke --function-name $npm_package_config_functionName --region $npm_package_config_region --payload file://serverless/api-gateway-event.json lambda-invoke-response.json && cat lambda-invoke-response.json",
    "create-bucket": "aws s3 mb s3://$npm_package_config_s3BucketName --region $npm_package_config_region",
    "delete-bucket": "aws s3 rb s3://$npm_package_config_s3BucketName --region $npm_package_config_region",
    "package": "aws cloudformation package --template ./serverless/cloudformation.yaml --s3-bucket $npm_package_config_s3BucketName --output-template packaged-sam.yaml --region $npm_package_config_region",
    "deploy": "aws cloudformation deploy --template-file packaged-sam.yaml --stack-name $npm_package_config_cloudFormationStackName --capabilities CAPABILITY_IAM --region $npm_package_config_region --parameter-overrides ApiKeyValue=${API_KEY_VALUE}",
    "package-deploy": "npm run package && npm run deploy",
    "delete-stack": "aws cloudformation delete-stack --stack-name $npm_package_config_cloudFormationStackName --region $npm_package_config_region",
    "setup": "npm install && (aws s3api get-bucket-location --bucket $npm_package_config_s3BucketName --region $npm_package_config_region || npm run create-bucket) && npm run build-package-deploy"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.0.0",
    "@turf/buffer": "^5.1.5",
    "@turf/turf": "^5.1.6",
    "@types/mapbox__polyline": "^1.0.0",
    "async": "^2.6.2",
    "aws-serverless-express": "^3.3.3",
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-mongo": "^2.0.3",
    "csv-parse": "^2.5.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "lodash": "^4.17.10",
    "lusca": "^1.6.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/async": "^2.4.1",
    "@types/axios": "^0.14.0",
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.35",
    "@types/connect-mongo": "^0.0.35",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.1",
    "@types/jest": "^22.2.3",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.121",
    "@types/lusca": "^1.5.0",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^4.7.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.6.42",
    "@types/nodemailer": "^4.6.6",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "^7.0.6",
    "@types/supertest": "^2.0.7",
    "@types/winston": "^2.4.4",
    "axios-mock-adapter": "^1.16.0",
    "chai": "^4.2.0",
    "concurrently": "^3.6.1",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "parcel-bundler": "^1.11.0",
    "sinon": "^7.2.4",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.0",
    "ts-mockito": "^2.3.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.12.1",
    "typescript": "^2.7.2"
  }
}
