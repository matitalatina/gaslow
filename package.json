{
  "name": "gaslow",
  "version": "0.2.1",
  "description": "Italian API price gas stations",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Mattia Natali",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "serve": "node --enable-source-maps dist/server.js",
    "build": "parcel build src/server.ts",
    "build-serverless": "parcel build src/lambda.ts",
    "lint:fix": "eslint src/ test/ --fix",
    "pretty:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "parcel watch --no-hmr src/server.ts",
    "watch-test": "npm run test -- --watchAll",
    "test": "jest --forceExit --coverage --detectOpenHandles",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "check-ts": "tsc --noEmit",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "targets": {
    "default": {
      "context": "node"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.2.1",
    "@turf/buffer": "^7.2.0",
    "@turf/helpers": "^7.2.0",
    "@types/mapbox__polyline": "^1.0.5",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "compression": "^1.8.0",
    "csv-parse": "^5.5.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "html-entities": "^2.5.2",
    "inversify": "^6.2.2",
    "inversify-express-utils": "^6.4.5",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongodb": "^6.5.0",
    "mongoose": "^8.2.1",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@jest/globals": "^29.7.0",
    "@parcel/transformer-typescript-tsc": "^2.13.3",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.202",
    "@types/moment": "^2.13.0",
    "axios-mock-adapter": "^2.1.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "parcel": "^2.12.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.1.2",
    "ts-mockito": "^2.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  }
}
